/* global wp, wpcomGutenbergMedia */

( function () {
	if (
		typeof wp === 'object' && wp &&
		typeof wpcomGutenbergMedia === 'object' && wpcomGutenbergMedia
	) {
		/**
		 * Add a filter that sets the cross-origin value for media files requests.
		 * It's necessary for Gutenberg's image editing to work in WPCOM.
		 */
		wp.hooks.addFilter(
			'media.crossOrigin',
			'gutenberg-wpcom',
			function ( crossOrigin, imgSrc ) {
				if ( 0 === imgSrc.indexOf( wpcomGutenbergMedia.blogFilesUrl ) ) {
					// Only set crossOrigin for images served for the corresponding site.
					return wpcomGutenbergMedia.crossOriginValue;
				}
				return crossOrigin;
			}
		);
	}
} )();
;
